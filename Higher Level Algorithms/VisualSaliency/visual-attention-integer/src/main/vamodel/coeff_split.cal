package main.vamodel;

import std.header.Parameter.*;

actor coeff_split() int coeffStream ==> int coeff_L, int coeff_H:
	bool L_dir := true;
	//int count
	split_L: action coeffStream:[val] ==>  coeff_L:[val_L], coeff_H:[val_H]
	var int val_L,
		int val_H
	do
		if(L_dir=true)then
			val_L := val;
			L_dir := false;
		else
			val_H := val;
			L_dir := true;
		end
	end 
    	
end